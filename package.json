{
  "name": "@fastify-modular/request",
  "version": "0.6.5",
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "types": "./types/index.d.ts",
      "require": "./cjs/index.js"
    }
  },
  "files": [
    "cjs/**/*.js",
    "cjs/package.json",
    "esm/**/*.js",
    "esm/package.json",
    "types/**/*.d.ts"
  ],
  "scripts": {
    "build": "rimraf \"{cjs,esm,types}/**/*.{js,js.map,d.ts,d.ts.map,tsbuildinfo}\" && tsc -b ./cjs/tsconfig.json ./esm/tsconfig.json ./types/tsconfig.json",
    "watch": "rimraf \"{cjs,esm,types}/**/*.{js,js.map,d.ts,d.ts.map,tsbuildinfo}\" && tsc -b ./cjs/tsconfig.json ./esm/tsconfig.json ./types/tsconfig.json --watch",
    "test": "tap --ts ./test/05-*.ts",
    "postversion": "npm publish",
    "prepack": "npm run build"
  },
  "keywords": [
    "typescript",
    "fastify",
    "fastify-modular"
  ],
  "author": "egoavara",
  "license": "MIT",
  "dependencies": {
    "events": "^3.3.0",
    "form-data": "^4.0.0",
    "qs": "^6.11.0",
    "uuid": "^8.3.2",
    "websocket": "^1.0.34"
  },
  "devDependencies": {
    "@fastify-modular/kafka": "^0.1.0",
    "@fastify-modular/route": "^0.8.0",
    "@types/node": "^18.0.5",
    "@types/qs": "^6.9.7",
    "@types/tap": "^15.0.7",
    "@types/uuid": "^8.3.4",
    "@types/websocket": "^1.0.5",
    "fastify": "^4.2.0",
    "fastify-modular": "^0.8.6",
    "pito": "^0.4.2",
    "rimraf": "^3.0.2",
    "tap": "^16.3.0",
    "ts-node": "^10.7.0",
    "typescript": "4.7.2"
  },
  "peerDependencies": {
    "@fastify-modular/route": "^0.8.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}