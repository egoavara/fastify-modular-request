{
  "name": "fastify-modular-request",
  "version": "0.4.1",
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "require": "./cjs/index.js"
    }
  },
  "files": [
    "cjs/**/*.js",
    "cjs/**/*.d.ts",
    "cjs/package.json",
    "esm/**/*.js",
    "esm/**/*.d.ts",
    "esm/package.json"
  ],
  "scripts": {
    "build": "npm run \"build:cjs\" & npm run \"build:esm\"",
    "build:cjs": "rimraf \"cjs/**/*.{js,js.map,d.ts,d.ts.map}\" && tsc -p ./cjs/tsconfig.json",
    "build:esm": "rimraf \"esm/**/*.{js,js.map,d.ts,d.ts.map}\" && tsc -p ./esm/tsconfig.json",
    "watch": "npm run \"watch:cjs\" & npm run \"watch:esm\"",
    "watch:cjs": "rimraf \"cjs/**/*.{js,js.map,d.ts,d.ts.map}\" && tsc --watch -p ./cjs/tsconfig.json",
    "watch:esm": "rimraf \"esm/**/*.{js,js.map,d.ts,d.ts.map}\" && tsc --watch -p ./esm/tsconfig.json",
    "test": "tap --ts ./test/03-ws.ts",
    "prepack": "npm run build"
  },
  "keywords": [
    "typescript",
    "fastify",
    "fastify-modular"
  ],
  "author": "egoavara",
  "license": "MIT",
  "dependencies": {
    "@types/event-source-polyfill": "^1.0.0",
    "axios": "^0.27.2",
    "both-sse": "^0.1.1",
    "form-data": "^4.0.0",
    "isomorphic-ws": "^4.0.1",
    "qs": "^6.10.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/eventsource": "^1.1.8",
    "@types/node": "^17.0.33",
    "@types/qs": "^6.9.7",
    "@types/tap": "^15.0.7",
    "@types/uuid": "^8.3.4",
    "@types/ws": "^8.5.3",
    "fastify": "^3.29.0",
    "fastify-modular": "^0.4.0",
    "fastify-modular-route": "^0.4.0",
    "pito": "^0.1.0",
    "rimraf": "^3.0.2",
    "tap": "^16.2.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  },
  "optionalDependencies": {
    "ws": "^8.6.0"
  },
  "peerDependencies": {
    "fastify-modular-route": "^0.4.0"
  }
}