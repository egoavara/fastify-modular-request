{
  "name": "fastify-modular-request",
  "version": "0.6.2",
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "require": "./cjs/index.js"
    }
  },
  "files": [
    "cjs/**/*.js",
    "cjs/**/*.d.ts",
    "cjs/package.json",
    "esm/**/*.js",
    "esm/**/*.d.ts",
    "esm/package.json"
  ],
  "scripts": {
    "build": "npm run \"build:cjs\" & npm run \"build:esm\"",
    "build:cjs": "rimraf \"cjs/**/*.{js,js.map,d.ts,d.ts.map}\" && tsc -p ./cjs/tsconfig.json",
    "build:esm": "rimraf \"esm/**/*.{js,js.map,d.ts,d.ts.map}\" && tsc -p ./esm/tsconfig.json",
    "watch": "npm run \"watch:cjs\" & npm run \"watch:esm\"",
    "watch:cjs": "rimraf \"cjs/**/*.{js,js.map,d.ts,d.ts.map}\" && tsc --watch -p ./cjs/tsconfig.json",
    "watch:esm": "rimraf \"esm/**/*.{js,js.map,d.ts,d.ts.map}\" && tsc --watch -p ./esm/tsconfig.json",
    "test": "tap --ts",
    "prepack": "npm run build"
  },
  "keywords": [
    "typescript",
    "fastify",
    "fastify-modular"
  ],
  "author": "egoavara",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.27.2",
    "both-sse": "^0.1.1",
    "form-data": "^4.0.0",
    "isomorphic-ws": "^5.0.0",
    "qs": "^6.10.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/node": "^17.0.33",
    "@types/qs": "^6.9.7",
    "@types/tap": "^15.0.7",
    "@types/uuid": "^8.3.4",
    "@types/ws": "^8.5.3",
    "@types/eventsource": "^1.1.8",
    "fastify": "^4.2.0",
    "fastify-modular": "^0.5.0",
    "fastify-modular-route": "^0.6.7",
    "pito": "^0.3.3",
    "rimraf": "^3.0.2",
    "tap": "^16.2.0",
    "ts-node": "^10.7.0",
    "typescript": "4.7.2"
  },
  "optionalDependencies": {
    "ws": "^8.8.0"
  },
  "peerDependencies": {
    "fastify-modular-route": "^0.6.7"
  }
}
